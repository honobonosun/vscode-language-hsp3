{
  "name": "language-hsp3",
  "publisher": "Honobono",
  "displayName": "language-hsp3",
  "description": "language-hsp3 for VSCode",
  "version": "0.0.0",
  "license": "MIT",
  "repository": "https://github.com/honobonosun/vscode-language-hsp3",
  "engines": {
    "vscode": "^1.33.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "hsp3",
        "aliases": ["HSP3", "hsp3", "HSP", "hsp"],
        "extensions": [".hsp",".hsp.i",".as",".as.i"],
        "configuration": "./language-configuration.json"
      },
      {
        "id": "hs",
        "aliases": ["hs"],
        "extensions": [".hs"]
      }
    ],
    "grammars": [
      {
        "language": "hsp3",
        "scopeName": "source.hsp3",
        "path": "./syntaxes/hsp3.tmLanguage.json"
      },
      {
        "language": "hs",
        "scopeName": "text.hs",
        "path": "./syntaxes/hs.tmLanguage.json"
      }
    ]
  }
}